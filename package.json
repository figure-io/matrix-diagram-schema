    "valid",
    "validate",
    "validator",
    "validation",
    "template",
    "specification",
    "spec"
  ],
  "bugs": {
    "url": "https://github.com/figure-io/matrix-diagram-spec/issues"
  },
  "dependencies": {
    "is-my-json-valid": "^2.4.1"
  },
  "devDependencies": {
    "chai": "1.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "^2.5.10",
    "jshint-stylish": "^1.0.0",
